{
  "name": "n8n-nodes-zadarma",
  "version": "1.0.1",
  "description": "Zadarma API integration node for n8n - reliable authentication based on working Python implementation",
  "keywords": [
    "n8n-community-node-package",
    "zadarma",
    "telephony",
    "api",
    "call-recording",
    "voip"
  ],
  "license": "MIT",
  "homepage": "https://github.com/zerox9dev/n8n-nodes-zadarma#readme",
  "author": {
    "name": "zerox9dev",
    "email": "zerox9dev@icloud.com",
    "url": "https://github.com/zerox9dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zerox9dev/zadarma-api"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ZadarmaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Zadarma/Zadarma.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.22",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {}
}